	public boolean isLecture() {
		while(!lecture) {
			for(int i=0;i<data.length;i++) {
				if(data[i].equals("Lecture")) {
					lecture = true;
					break;
				}
			}
		}
		return lecture;
	}
	
	public String toString() {
		StringBuilder SB = new StringBuilder();		
		return SB.toString();
	}
